      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>if !pause
{   
        
    if abs(xspeed)&lt;0.1*global.delta
    {
    xspeed=0;
    }
    if abs(yspeed)&lt;0.1*global.delta
    {
    yspeed=0;
    }
    if abs(xspeed)&gt;max_speed
    {
    xspeed=max_speed*(xspeed/abs(xspeed));
    }
    if abs(yspeed)&gt;max_speed
    {
    yspeed=max_speed*(yspeed/abs(yspeed));
    }

    var pre_xspeed=xspeed;
    var pre_yspeed=yspeed;
    if knockback_speed&lt;&gt;0
    {
        xspeed+=lengthdir_x(knockback_speed,knockback_dir);
        yspeed+=lengthdir_y(knockback_speed,knockback_dir);
    }
    if melee_speed&lt;&gt;0
    {
            xspeed=lengthdir_x(melee_speed,melee_direction);
            yspeed=lengthdir_y(melee_speed,melee_direction);     
    }
    
    if (xspeed!=0)||(yspeed!=0)
    {
    cellsize=global.path_grid_cellsize;
    }
    
    var moveFactor=min(6,20*global.delta);
    
    if (xspeed!=0)
    {
        dir=(xspeed/abs(xspeed)); //indicates polarity of xspeed
        i=abs(xspeed);
        while i&gt;0
        {
            if (control=='player')
            {
                if GridIsFree(global.player_grid,x+(6*dir),y, cellsize)
                {
                x+=moveFactor*dir;
                }
                else if GridIsFree(global.player_grid,x,y+6, cellsize)
                {
                y+=moveFactor;
                }
                else if GridIsFree(global.player_grid,x,y-6, cellsize)
                {
                y-=moveFactor;
                }
            }
            else
            {
                if GridIsFree(global.path_grid,x+(6*dir),y, cellsize)
                {
                x+=moveFactor*dir;
                }
                else if GridIsFree(global.path_grid,x,y+6, cellsize)
                {
                y+=moveFactor;
                }
                else  if GridIsFree(global.path_grid,x,y-6, cellsize)
                {
                y+=moveFactor;
                }
            }
        i-=1;
        }
    }
    if (yspeed!=0)
    {
        dir=(yspeed/abs(yspeed)); //indicates polarity of xspeed
        i=abs(yspeed);
        while i&gt;0
        {
            if (control=='player')
            {
                if GridIsFree(global.player_grid,x,y+(6*dir), cellsize)
                {
                y+=moveFactor*dir;
                }
                else if GridIsFree(global.player_grid,x+6,y, cellsize)
                {
                x+=moveFactor;
                }
                else if GridIsFree(global.player_grid,x-6,y, cellsize)
                {
                x-=moveFactor;
                }
            }
            else
            {
                if 0==mp_grid_get_cell(global.path_grid,(x) div cellsize,(y+(6*dir)) div cellsize)
                {
                y+=min(6,6*global.delta)*dir;
                }
                else if 0==mp_grid_get_cell(global.path_grid,(x+6) div cellsize,(y+(6*dir)) div cellsize)
                {
                x+=min(6,6*global.delta);
                }
                else if 0==mp_grid_get_cell(global.path_grid,(x-6) div cellsize,(y+(6*dir)) div cellsize)
                {
                x-=min(6,6*global.delta);
                }
            }
        i-=1;
        }
    }
    
    xspeed=pre_xspeed;
    yspeed=pre_yspeed;
    
    if(abs(xspeed)&gt;0||abs(yspeed)&gt;0)
    {
    last_direction=point_direction(0,0,xspeed,yspeed);
    }

}

</string>
          </argument>
