      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>if !pause
{
    if action_state=='firing'
    {
           var exit_state = false;
            
            switch shoot_substate
            {
            case 'left':
                if !firing_L  
                {
                    if (firearm_L[1]&gt;0)
                    {
                    action_state='aiming';
                    }
                    else
                    {
                    exit_state=true;
                    }
                
                }
            break;
            case 'right':
                if !firing_R  
                {
                    if (firearm_R[1]&gt;0)
                    {
                    action_state='aiming';
                    }
                    else
                    {
                    exit_state=true;
                    }
                
                }
            break;
            case 'both':
                if !firing_L &amp;&amp; !firing_R 
                {
                
                    if (firearm_L[1]&gt;0 || firearm_R[1]&gt;0)
                    {
                    action_state='aiming';
                    }
                    else
                    {
                    exit_state=true;
                    }

                }
            break;
            case 'long':
                if !firing_R  
                {
                    if (firearm_R[1]&gt;0)
                    {
                    action_state='aiming';
                    }
                    else
                    {
                    exit_state=true;
                    }
                
                }
            break;
            }
            if exit_state
            {
            movement_state='idle';
            
                if reload_style=='toss'
                {
                action_trigger='toss';
                }
                else
                {
                action_trigger='idle';
                    if firearm_L[0]&gt;-1  //if firing has ended, readd both empty firearms to the inventory.
                    {
                        firearm_inventory_add(firearm_L[0],firearm_L[1]);
                        firearm_L=create_array(-1,0);
                    }
                    if firearm_R[0]&gt;-1
                    {
                        firearm_inventory_add(firearm_R[0],firearm_R[1]);
                        firearm_R=create_array(-1,0);
                    }
                }
           if (currentAnim!=-1){ds_map_destroy(currentAnim)}
            currentAnim=-1;
            inst_body.anim_countdown=-1;
            inst_armR.anim_countdown=-1;
            inst_armL.anim_countdown=-1;
            inst_head.anim_countdown=-1;
            inst_weaponL.anim_countdown=-1;
            inst_weaponR.anim_countdown=-1;
            inst_hat.anim_countdown=-1;
            }


    }
}
